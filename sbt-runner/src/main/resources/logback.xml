<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{ISO8601} %-5level %logger{36}:%line %X{sourceThread} - %msg%n</pattern>
    </encoder>
  </appender>
  <root level="INFO">
      <appender-ref ref="STDOUT" />
  </root>
</configuration>
