# --- Section: Help Window ---
msgid "help.title"
msgstr "Help about Scastie"

msgid "help.description"
msgstr "Scastie is an interactive playground for Scala with support for sbt configuration."

# The translation of `keyboard shortcuts` must remain inside the curly brackets.
msgid "help.sublime_support"
msgstr "Scastie editor supports Sublime Text {keyboard shortcuts}."

msgid "help.save"
msgstr "Save"

msgid "help.save_description"
msgstr "Run and save your code."

msgid "help.new_description"
msgstr "Removes all your code lines from the current editor instance and resets sbt configuration."

msgid "help.clear_messages_description"
msgstr "Removes all messages from the current editor instance."

# The translation of `configuration section` must remain inside the curly brackets.
msgid "help.format_description"
msgstr "The code formatting is done by scalafmt. You can configure the formatting with comments in your code. Read the {configuration section}."

# The code must remain inside the curly brackets.
msgid "help.worksheet_description"
msgstr "Enabled by default, the Worksheet Mode gives the value and the type of each line of your program. You can also add HTML blocks such as: {html"<h1>Hello</h1>"} to render it next to the declaration."

msgid "help.worksheet_packages_warning"
msgstr "In Worksheet Mode you cannot use packages. If you want to use it, turn off the Mode and add a main method and println statements."

msgid "help.download_description"
msgstr "Create a zip package with sbt configuration for current snippet."

msgid "help.embed_description"
msgstr "Create an url embeddable in external web pages."

msgid "help.console_description"
msgstr "You can see your code's output in the Scastie's console."

msgid "help.build_settings_description"
msgstr "In Build Settings you can change the Scala version and add libraries, choose your desired target and even add your own custom sbt configuration."

msgid "help.snippets_title"
msgstr "User's Code Snippets"

msgid "help.snippets_description"
msgstr "Your saved code fragments will appear here and you'll be able to delete or share them."

# The translation of `Gitter` must remain inside the curly brackets.
msgid "help.feedback_description"
msgstr "You can join our {Gitter} channel and send issues."

msgid "help.buildinfo"
msgstr "BuildInfo"

# The repository name `scalacenter/scastie` must remain inside the curly brackets.
msgid "help.github_info"
msgstr "It's available on Github at {scalacenter/scastie}."

